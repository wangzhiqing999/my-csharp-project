@model IEnumerable<MyDatabaseDoc.Model.Table>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TableName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TableComment)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TableName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TableComment)
        </td>
        <td>
            @Html.ActionLink("明细", "Details", new {  id=item.TableName })
        </td>
    </tr>
}

</table>
