@model MyBallot.Model.BallotMaster


    
<h3> @Model.BallotTypeCode : @Model.BallotTypeData.BallotTypeName </h3>
    
    
<ul>
    @foreach (var subItem in Model.BallotDetailList)
    {
        <li>
            <span> @subItem.BallotTypeDetailCode : @subItem.BallotTypeDetailData.BallotTypeDetailTitle </span>

            <span> 投票数：@subItem.BallotCount </span>

            <span> 投票百分比：@subItem.BallotPercent.ToString("00.00")% </span>

            <a href="javascript:void(0);" onclick="DoBallot( @subItem.BallotMasterID,  @subItem.BallotDetailID ) "> 投票 </a>
        </li>
    }
</ul>



