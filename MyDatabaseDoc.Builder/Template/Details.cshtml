<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>数据库文档</title>
</head>
<body>
    <table class="table">
        <tr>
            <th>
                列名
            </th>
            <th>
                数据类型
            </th>
            <th>
                是否允许为空
            </th>
            <th>
                备注
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.ColumnName
                </td>
                <td>
                    @item.DataType
                </td>
                <td>
                    @item.IsNullable
                </td>
                <td>
                    @item.ColumnComment
                </td>
            </tr>
        }

    </table>
</body>
</html>

